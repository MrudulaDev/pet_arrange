{
   "swagger":"2.0",
   "host":"localhost:8000",
   "basePath":"/api/pets_core/",
   "info":{
      "version":"1.0.0",
      "title":"Pet Arrange",
      "description":"Connects Rescued Pets in Shelters with Prospective Adopters"
   },
   "schemes":[
      "https",
      "http"
   ],
   "consumes":[
      "application/json"
   ],
   "produces":[
      "application/json"
   ],
   "securityDefinitions":{
      "oauth":{
         "tokenUrl":"http://auth.ibtspl.com/oauth2/",
         "flow":"password",
         "scopes":{
            "read":"read users",
            "write":"create users",
            "update":"update users",
            "delete":"delete users",
            "superuser":"super user permission"
         },
         "type":"oauth2"
      }
   },
   "definitions":{
      "CommonHttpExceptionResponse":{
         "type":"object",
         "properties":{
            "response":{
               "type":"string"
            },
            "http_status_code":{
               "type":"integer"
            }
         },
         "required":[
            "response",
            "http_status_code"
         ]
      },
      "PetResponse":{
         "type":"object",
         "properties":{
            "pet_id":{
               "type":"integer",
               "description":"Id of the pet"
            },
            "name":{
               "type":"string",
               "description":"The name of the pet"
            },
            "age":{
               "type":"integer",
               "description":"The age of the pet"
            },
            "pet_category":{
               "type":"string",
               "description":"what animal is the pet?",
               "enum":[
                  "DOG",
                  "CAT",
                  "PARROT",
                  "RABBIT",
                  "OTHERS"
               ]
            },
            "pet_size":{
               "type":"string",
               "description":"The size of the pet (e.g., small, medium, large)",
               "enum":[
                  "SMALL",
                  "MEDIUM",
                  "LARGE"
               ]
            },
            "gender":{
               "type":"string",
               "description":"The gender of the pet",
               "enum":[
                  "MALE",
                  "FEMALE"
               ]
            }
         },
         "required":[
            "pet_id",
            "name",
            "age",
            "pet_category",
            "pet_size",
            "gender"
         ]
      },
      "AdoptionRequestObject":{
         "type":"object",
         "properties":{
            "request_id":{
               "type":"integer",
               "description":"Id of the request"
            },
            "request_status":{
               "type":"string",
               "description":"status of the request",
               "enum":[
                  "OPEN",
                  "APPROVED",
                  "CLOSED"
               ]
            },
            "pet_id":{
               "type":"integer",
               "description":"Id of the pet"
            },
            "adopter_id":{
               "type":"integer",
               "description":"Id of the adopter"
            },
            "requested_at":{
               "type":"string",
               "format": "date-time",
               "description":"timestamp for when the request was created"
            }
         },
         "required":[
            "request_id",
            "request_status",
            "pet_id",
            "adopter_id",
            "requested_at"
          ]
      }
