         "schema":{
            "type":"object",
            "properties":{
               "all_pets":{
                  "type":"array",
                  "items":{
                     "$ref":"#/definitions/PetResponse"
                  }
               }
            }
         }
      },
      "UpdatePetValidResponse":{
         "description":"Updated Successfully",
